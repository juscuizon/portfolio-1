mixin navigation-links
	ul
		li: a(href="/..") HOME
		li: a(href="/categories/technology.html") TECHNOLOGY
		li: a(href="/categories/finance.html") FINANCE
		li: a(href="/categories/fitness.html") FITNESS
		li: a(href="/categories/food.html") FOOD
		li: a(href="/categories/travel.html") TRAVEL
		li: a(href="#") SERVICES
		li: a(href="/contact") CONTACT
		li: a(href="/about") ABOUT

.mobile-header-bar
	span#mobile-navigation-button.mobile-navigation-button


.mobile-navigation
	+navigation-links

